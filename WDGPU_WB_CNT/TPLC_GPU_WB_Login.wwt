     code : |1+
      // Summary: Indicates whether the SSL mode must be enabled
      procedure bMustActivateSSL()
      
      // if the SSL must not be enabled
      if SET_GPU_CNT.gbActivateSSL=False then
      	return False
      end
      
      // No: if we are in SSL already
      extern location
      if location.protocol = "https" then
      	return False
      end
      
      // NO: If the configuration does not require it
      // @@@ Always for the moment
      return True
     type : 458752
     role : N
   -
     name : bCurrentConnection
     procedure_id : 1449464528562442710
     type_code : 14
     code : |1+
      // Summary: Find out whether a user is currently connected
      procedure bCurrentConnection()
      
      // retrieve the connected user
      sUsername is string UNICODE=gpwGetUserInfo(gpwInfoLogin)
      
      // if no user is connected yet
      if sUsername="" _or_ sUsername=gpwVisitor then
      	return False
      end
      
      // if a user is currently connected
      return True
     type : 458752
   -
     name : AdaptConnectionCaption
     procedure_id : 1449464528562508246
     type_code : 14
     code : |1+
      // Summary: Adapts the caption of the Connection button
      procedure AdaptConnectionCaption()
      
      // if a user is currently connected
      if bCurrentConnection() then
      	// proposes the disconnection
      	LINK_Log_in.Caption=<ยง$0000ยง>
      	
      	// if no user is connected yet
      else
      	// proposes the connection
      	LINK_Log_in.Caption=<ยง$0001ยง>
      end
      
